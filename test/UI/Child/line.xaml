<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="test.line"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Width="1272" Height="598">

	<Grid x:Name="LayoutRoot" Height="600" Width="1272">
		<Rectangle HorizontalAlignment="Center" Height="600" Margin="-14,3,-14,-3" RadiusY="7.5" RadiusX="7.5" StrokeThickness="9" VerticalAlignment="Center" Width="1300" Fill="White">
			<Rectangle.Stroke>
				<RadialGradientBrush>
					<GradientStop Color="#FF094682" Offset="0"/>
					<GradientStop Color="#FFD1E9FF" Offset="1"/>
				</RadialGradientBrush>
			</Rectangle.Stroke>
		</Rectangle>
		<StackPanel HorizontalAlignment="Left" Height="580" Margin="10,10,0,0" VerticalAlignment="Top" Width="1252"/>
	</Grid>
</UserControl>